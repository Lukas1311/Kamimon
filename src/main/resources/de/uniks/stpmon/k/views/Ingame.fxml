<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<StackPane fx:id="ingameStack" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="de.uniks.stpmon.k.controller.IngameController">
    <VBox fx:id="ingame" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="400.0" prefWidth="600.0">
        <Text fx:id="inGameText" strokeType="OUTSIDE" strokeWidth="0.0" text="INGAME">
            <font>
                <Font size="96.0"/>
            </font>
        </Text>
    </VBox>
</StackPane>
