<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>
<?import java.lang.String?>
<StackPane fx:id="starterPane" prefHeight="332.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/11.0.1"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.stpmon.k.controller.StarterController"
           stylesheets="@css/ingameStyle.css">
    <styleClass>
        <String fx:value="ingame" />
    </styleClass>
    <ImageView fx:id="starterBackground" fitHeight="332.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true" />
    <VBox alignment="TOP_CENTER" prefHeight="332.0" prefWidth="250.0">
        <Label fx:id="monsterNameLabel" alignment="CENTER" styleClass="monster-information-name">
            <VBox.margin>
                <Insets top="30.0" />
            </VBox.margin>
        </Label>
        <ImageView fx:id="starterImage" fitHeight="148.0" fitWidth="180.0" pickOnBounds="true" preserveRatio="true">
            <VBox.margin>
                <Insets bottom="30.0" />
            </VBox.margin>
        </ImageView>
        <TextFlow prefHeight="58.0" prefWidth="210.0">
            <VBox.margin>
                <Insets left="25.0" right="25.0" />
            </VBox.margin>
            <Text fx:id="descriptionText" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
        </TextFlow>
        <StackPane.margin>
            <Insets />
        </StackPane.margin>
    </VBox>
</StackPane>
