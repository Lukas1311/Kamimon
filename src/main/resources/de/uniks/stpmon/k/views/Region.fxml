<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<VBox fx:id="regionVBox" alignment="CENTER" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.uniks.stpmon.k.controller.lobby.RegionController">


    <Text fx:id="regionNameText" strokeType="OUTSIDE" strokeWidth="0.0" text="Text"/>

    <BorderPane fx:id="imageWrappingPane" styleClass="region-pane-border" stylesheets="@css/generalStyle.css">
        <center>
            <ImageView fx:id="regionImage" pickOnBounds="true" preserveRatio="true"/>
        </center>
    </BorderPane>
</VBox>
